<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extensions">

  <schema path="/org/gnome/shell/extensions/web-search-dialog/" id="org.gnome.shell.extensions.web-search-dialog">

    <key type="as" name="open-web-search-dialog">
      <default><![CDATA[['<Super>space']]]></default>
      <summary>Open web search.</summary>
    </key>

    <key type="s" name="search-history-data">
      <default>"[]"</default>
    </key>

    <key type="s" name="search-engines">
      <default>
        <![CDATA[
          '{
             "1":{
                "name":"Google",
                "keyword":"g",
                "url":"http://google.com/search?q={term}",
                "suggestions_engine":"google_suggestions.js",
                "enable_suggestions":true,
                "enable_helpers":true,
                "allowed_helpers": ["duckduckgo_helper.js", "freebase_helper.js", "wikipedia_helper.js"]
             },
             "2":{
                "name":"DuckDuckGo",
                "keyword":"d",
                "url":"http://duckduckgo.com/?q={term}",
                "suggestions_engine":"default",
                "enable_suggestions":true,
                "enable_helpers":true,
                "allowed_helpers": ["duckduckgo_helper.js"]
             },
             "3":{
                "name":"Yandex",
                "keyword":"y",
                "url":"http://yandex.com/yandsearch?text={term}",
                "suggestions_engine":"default",
                "enable_suggestions":true,
                "enable_helpers":true,
                "allowed_helpers": ["duckduckgo_helper.js", "freebase_helper.js", "wikipedia_helper.js"]
             },
             "4":{
                "name":"Wikipedia",
                "keyword":"w",
                "url":"http://en.wikipedia.org/wiki/Special:Search?search={term}",
                "suggestions_engine":"default",
                "enable_suggestions":true,
                "enable_helpers":true,
                "allowed_helpers": ["wikipedia_helper.js"]
             },
             "5":{
                "name":"Bing",
                "keyword":"b",
                "url":"http://www.bing.com/search?q={term}",
                "suggestions_engine":"default",
                "enable_suggestions":true,
                "enable_helpers":true,
                "allowed_helpers": ["duckduckgo_helper.js", "freebase_helper.js", "wikipedia_helper.js"]
             },
             "6":{
                "name":"Imdb",
                "keyword":"i",
                "url":"http://www.imdb.com/find?s=all&q={term}",
                "suggestions_engine":"imdb_suggestions.js",
                "enable_suggestions":true,
                "enable_helpers":false,
                "allowed_helpers": ["freebase_helper.js"]
             }
          }'
        ]]>
      </default>
      <summary>Search engines.</summary>
    </key>

    <key type="b" name="enable-suggestions">
        <default>true</default>
        <summary>Enable suggetsions</summary>
    </key>

    <key type="s" name="default-suggestions">
        <default>'google_suggestions.js'</default>
    </key>

    <key type="i" name="suggestions-delay">
        <default>150</default>
    </key>

    <key type="i" name="max-suggestions">
        <default>4</default>
    </key>

    <key type="b" name="enable-duckduckgo-helper">
        <default>true</default>
    </key>

    <key type="i" name="helper-delay">
        <default>500</default>
    </key>

    <key type="i" name="helper-position">
      <default>1</default>
    </key>

    <key type="b" name="enable-history-suggestions">
      <default>true</default>
      <summary>Enable history suggestions.</summary>
    </key>

    <key type="i" name="history-limit">
      <default>500</default>
    </key>

    <key type="i" name="max-history-suggestions">
        <default>2</default>
    </key>

    <key type="i" name="default-search-engine">
      <default>1</default>
    </key>

    <key type="b" name="enable-open-url">
        <default>true</default>
    </key>

    <key type="s" name="open-url-label">
      <default>"Open URL"</default>
    </key>

    <key type="s" name="open-url-keyword">
      <default>"url"</default>
    </key>

  </schema>
</schemalist>
